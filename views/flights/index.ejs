<%- include('../partials/header') %>

    <table>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight Number</th>
        <th>Departure Date</th>
      </tr>
    </thead>
    <tbody>

      <% flights.forEach(m => { %>
        <tr>
          <td><%= m.airline %></td>
          <td><%= m.airport %></td>
          <td><%= m.flightNo %></td>
          <td><%= m.departs%></td>
          <td><a href="/flights/<%= m._id %>">Details</a></td>
        </tr>
        <% }) %>
    </tbody>
  </table>

  <%- include('../partials/footer') %>
