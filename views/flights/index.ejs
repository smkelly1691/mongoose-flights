<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <title>Mongoose Flights</title>
</head>
<body>
  <h1>Flight List</h1>
  <table>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight Number</th>
        <th>Departure Date</th>
      </tr>
    </thead>
    <tbody>

      <% flights.forEach(m => { %>
        <tr>
          <td><%= m.airline %></td>
          <td><%= m.airport %></td>
          <td><%= m.flightNo %></td>
          <td><%= m.departs%></td>
        </tr>
        <% }) %>
    </tbody>
  </table>
</body>
</html>
